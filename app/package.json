{
  "private": true,
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "build": "craco build",
    "build:docker": "craco --max_old_space_size=4096 build",
    "build:legacy": "NODE_OPTIONS=--openssl-legacy-provider craco build",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "eslint . --ext ts,tsx",
    "lint:strict": "eslint . --ext ts,tsx -c ./.eslintrc-strict.json",
    "start": "craco start",
    "start:legacy": "NODE_OPTIONS=--openssl-legacy-provider craco start",
    "test": "jest",
    "compile": "npx tsc --noEmit",
    "e2e": "npx cypress run",
    "e2e:gui": "npx cypress open",
    "start:ci": "serve -s build",
    "e2e:ci": "start-server-and-test start:ci http://localhost:3000 e2e"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@jet-lab/e2e-react-adapter": "^0.1.3",
    "@jet-lab/margin": "^0.2.1",
    "@jup-ag/react-hook": "^1.0.0-beta.29",
    "@project-serum/anchor": "^0.24.2",
    "@solana/spl-token": "^0.2.0",
    "@solana/wallet-adapter-react": "^0.15.8",
    "@solana/wallet-adapter-wallets": "^0.16.8",
    "@solana/web3.js": "^1.32.0",
    "antd": "^4.17.2",
    "apexcharts": "^3.35.2",
    "axios": "^0.27.2",
    "chai": "^4.3.6",
    "immutable-tuple": "^0.4.10",
    "logrocket": "^3.0.1",
    "react": "^17.0.2",
    "react-csv": "^2.2.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.1.1",
    "react-sparklines": "^1.7.0",
    "react-string-replace": "^1.0.0",
    "react-to-print": "^2.14.7",
    "recoil": "^0.7.4"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.37",
    "@types/react-csv": "^1.1.2",
    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-sparklines": "^1.7.2",
    "@types/react-transition-group": "^4.4.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "craco-less": "^1.20.0",
    "cypress": "^10.3.1",
    "cypress-wait-until": "^1.7.2",
    "eslint-config-react-app": "^6.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.5.1",
    "react-scripts": "^4.0.3",
    "source-map-loader": "^1.1.3",
    "start-server-and-test": "^1.14.0",
    "typescript": "4.4.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
