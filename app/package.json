{
  "private": true,
  "name": "app",
  "version": "0.1.0",
  "scripts": {
    "build": "craco build",
    "build:docker": "craco --max_old_space_size=4096 build",
    "build:legacy": "NODE_OPTIONS=--openssl-legacy-provider craco build",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "eslint . --ext ts,tsx",
    "lint:strict": "eslint . --ext ts,tsx -c ./.eslintrc-strict.json",
    "start": "(trap 'kill 0' SIGINT; cd ../libraries/ts && tsc --watch & FORCE_COLOR=true npx craco start | cat)",
    "start:legacy": "(trap 'kill 0' SIGINT; cd ../libraries/ts && tsc --watch & FORCE_COLOR=true NODE_OPTIONS=--openssl-legacy-provider npx craco start | cat)",
    "test": "jest",
    "compile": "npx tsc --noEmit",
    "e2e": "npx cypress run",
    "e2e:gui": "npx cypress open",
    "start:ci": "serve -s build",
    "e2e:ci": "start-server-and-test start:ci http://localhost:3000 e2e"
  },
