{
  "name": "jet-app",
  "private": true,
  "scripts": {
    "build": "craco build",
    "fmt": "prettier --check .",
    "fmt:fix": "prettier --write .",
    "lint": "eslint . --ext ts,tsx",
    "start": "craco start",
    "start:legacy": "NODE_OPTIONS=--openssl-legacy-provider craco start",
    "debug": "cd ../jet-v2/libraries/ts && yarn compile && npm link && cd ../../../jet-app && npm link @jet-lab/margin && npm run start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "compile": "npx tsc --noEmit",
    "precommit": "npm run fmt:fix",
    "checkall": "npm run compile && npm run fmt:fix && npm run lint && npm run test",
    "e2e": "npx cypress run",
    "e2e:gui": "npx cypress open"
  },
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@dialectlabs/react": "^0.5.4",
    "@dialectlabs/react-ui": "^1.0.0-beta.14",
    "@jet-lab/e2e-react-adapter": "^0.1.1",
    "@jet-lab/jet-antd": "^0.4.28",
    "@jet-lab/margin": "^0.1.66",
    "@project-serum/anchor": "^0.24.2",
    "@pythnetwork/client": "^2.7.0",
    "@solana/buffer-layout": "^4.0.0",
    "@solana/buffer-layout-utils": "^0.2.0",
    "@solana/spl-token": "^0.1.8",
    "@solana/spl-token-latest": "npm:@solana/spl-token@^0.2.0",
    "@solana/wallet-adapter-base": "^0.9.7",
    "@solana/wallet-adapter-react": "^0.15.6",
    "@solana/wallet-adapter-wallets": "^0.16.8",
    "@solana/web3.js": "^1.41.4",
    "@types/bn.js": "^5.1.0",
    "antd": "^4.17.2",
    "bs58": "^4.0.1",
    "chai": "^4.3.6",
    "logrocket": "^3.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.4",
    "react-query": "^3.39.0",
    "react-router": "^6.1.1",
    "react-router-dom": "^6.1.1",
    "react-string-replace": "^1.1.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/bs58": "^4.0.1",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "@types/react-router": "^5.1.17",
    "@types/react-router-dom": "^5.3.2",
    "craco-less": "^1.20.0",
    "cypress": "^10.3.1",
    "cypress-wait-until": "^1.7.2",
    "eslint-config-react-app": "6.0.0",
    "prettier": "^2.5.1",
    "react-error-overlay": "^6.0.9",
    "react-scripts": "^4.0.3",
    "source-map-loader": "^1.1.3",
    "ts-node": "^10.8.2",
    "typescript": "^4.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
